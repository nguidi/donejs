<div id="userEstacionarPage" data-role="page">
    <div data-role="header">
        <div class="ui-btn-left">
            <a rel="back" data-icon="home" data-iconpos="notext" data-direction="reverse" class="ui-btn-left ui-btn ui-shadow ui-btn-corner-all ui-btn-icon-notext ui-btn-up-f" data-corners="true" data-shadow="true" data-iconshadow="true" data-wrapperels="span" data-theme="f" title="Home">
                <span class="ui-btn-inner ui-btn-corner-all">
                    <span class="ui-btn-text">
                        Home
                    </span>
                    <span class="ui-icon ui-icon-home ui-icon-shadow">
                        &nbsp;
                    </span>
                </span>
            </a>
        </div>
        <h1>CPI</h1>
        <div class="ui-btn-right">
            <a data-icon="gear" id="opciones">
                <span class="ui-btn-text">
                    <%= this.username %>
                </span>
            </a>
        </div>
    </div>
    <div id="mainEstacionar" data-role="content">
        
        <div id="alert"></div>

        <dl>			
            <dt>Municipio: </dt>
            <dd class="municipio"></dd>
        </dl>

        <div id="marcaSelect"> </div>

        <div data-role="fieldcontain" class="ui-field-contain ui-body ui-br">
            <label for="name" class="ui-input-text">
                Patente:
            </label>
            <input type="text" name="patente" id="patente">
        </div>

        <div id="tarifaSelect"> </div>

        

        <h2>Su saldo es: $ <%= this.saldo %></h2> 

        <a rel="back" data-role="button" data-inline="true">Cancelar</a>
        <a data-role="button" data-inline="true" id="estacionar" data-theme="b">Estacionar</a>

    </div>
</div>